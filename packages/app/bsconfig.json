{
  "name": "@demo/app",
  "version": "0.1.0",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
	"@demo/some-package",
	"@demo/shim-demo"
  ],
  "warnings": {
    "error" : "+101"
  },
  "namespace": true,
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript",
    "use-bs-dependencies": true,
    "importPath": "relative",
    "exportInterfaces": false,
    "module": "es6",
    "shims": {
      "Shimied": "Shimed.shim.ts"
    },
    "namespace": true
  }
}
